   "source": [
    "import cv2\n",
    "\n",
    "def color2gray(input_dir, output_dir):\n",
    "    img = cv2.imread(input_dir)#read image\n",
    "    cvtimg = img[:,:,1] #convert to gray scale\n",
    "    cv2.imwrite(output_dir, cvtimg) # save image to output_dir\n",
    "    return\n"
   ]
  },
